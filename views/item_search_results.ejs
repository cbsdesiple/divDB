<%- include("partials/header") %>

<h2> Search Results</h2>
<table class="table table-dark table-bordered">
  <thead>
    <tr>
      <th scope="col">Title</th>
      <th scope="col">Barcode</th>
      <th scope="col">Due Date<th>
    </tr>
  </thead>
  <tbody>
    <% for(let i = 0; i < items.length; i++){ %>
      <tr>
        <th scope="row"><a class="link" href="/items/<%=items[i].item_barcode%>"><%=items[i].item_title%></a></th>
        <th scope="row"><a class="link" href="/items/<%=items[i].item_barcode%>"><%=items[i].item_barcode%></a></th>
        <th scope="row"><a class="link" href="/items/<%=items[i].item_barcode%>"><%=items[i].item_due_date%></a></th>
      </tr>
      <% } %>
  </tbody>
</table>



<%- include("partials/footer") %>
